<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/meego/toolbar/panels/order</key>
      <applyto>/desktop/meego/toolbar/panels/order</applyto>
      <owner>mutter-meego</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[meego-panel-myzone,meego-panel-zones,meego-panel-applications,meego-panel-status,meego-panel-people,meego-panel-internet,meego-panel-banshee,meego-panel-devices,meego-power-icon,meego-panel-datetime,carrick,bluetooth-panel]</default>
      <locale name="C">
         <short>Order of panels on toolbar</short>
         <long>
		Determines the order of panels on the toolbar.
         </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/meego/always_small_screen_mode</key>
      <applyto>/desktop/meego/always_small_screen_mode</applyto>
      <owner>mutter-meego</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
         <short>Always use small screen mode</short>
         <long>
		Forces small screen mode to be used even if display size would
                normally imply bigger screen mode.
         </long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
